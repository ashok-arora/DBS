<style>
    .MyModal {
        position: fixed;
        display: flex;
        width: 100vw;
        height: 100vh;
        top: 0%;
        left: 0%;
        z-index: 10;
        align-items: center;
        justify-content: center;
        background-color: black;
        background-color: rgba(0, 0, 0, 0.8);
    }

    .ModalContent {
        display: flex;
        flex-direction: column;
        align-items: center;
        width: 400px;
        z-index: 11;
        border-radius: 1em;
        padding: 10px;
        background-color: sandybrown;
    }

    .Button {
        border-radius: 1em;
        padding: 10px 20px;
        background-color: lightgreen;
        text-align: center;
    }

    .Button:hover {
        cursor: pointer;
    }

    h3 {
        margin: 0;
    }
</style>
<div class="MyModal">
    <div class="ModalContent">
        <h2>Are you sure you want to logout?</h2>
        <div style="display: flex;">
            <!-- Enter Link for Yes here -->
            <div class="Button" style="margin-right: 20px;" onclick=changeToLastSelection()><h3>Yes</h2></div>
            <div class="Button" onclick="changeToLastSelection()"><h3>No</h3></div>
        </div>
    </div>
</div>